{
    "name": "heyday/silverstripe-colorpalette",
    "type": "silverstripe-vendormodule",
    "description": "A color palette picker for SilverStripe CMS",
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "silverstripe/framework": "^6",
        "silverstripe/vendor-plugin": "^2"
    },
    "autoload": {
        "psr-4": {
            "Heyday\\ColorPalette\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "3.x-dev"
        },
        "expose": [
            "css",
            "js"
        ]
    }
}
